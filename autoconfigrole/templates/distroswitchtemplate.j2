!
router eigrp NA-EIGRP
 !
 address-family ipv4 unicast autonomous-system 65510
  !
  af-interface Vlan2
   passive-interface
  exit-af-interface
  !
  af-interface Vlan3
   passive-interface
  exit-af-interface
  !
  af-interface Vlan4
   passive-interface
  exit-af-interface
  !
  af-interface Vlan5
   passive-interface
  exit-af-interface
  !
  af-interface Vlan6
   passive-interface
  exit-af-interface
  !
  af-interface Vlan7
   passive-interface
  exit-af-interface
  !
  af-interface Vlan99
   passive-interface
  exit-af-interface
  !
  topology base
  exit-af-topology
  network 10.22.0.0 0.0.255.255
  eigrp router-id {{ vars[short_hostname+'_ip_address'].eigrp.ipv4.router_id }}
 exit-address-family
 !
 address-family ipv6 unicast autonomous-system 65510
  !
  af-interface Vlan2
   passive-interface
