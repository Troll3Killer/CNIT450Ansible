  exit-af-interface
  !
  af-interface Vlan99
   passive-interface
  exit-af-interface
  !
  topology base
  exit-af-topology
  eigrp router-id {{ vars[short_hostname+'_ip_address'].eigrp.ipv6.router_id }}
